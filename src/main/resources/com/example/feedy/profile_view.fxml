<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="all_container" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.feedy.controllers.ProfileController">
   <children>
      <Label fx:id="usernameLabel" layoutX="160.0" layoutY="41.0" prefHeight="66.0" prefWidth="225.0" styleClass="profile_username" text="Label">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="bioLabel" layoutX="160.0" layoutY="107.0" text="Hello My name is jacer">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ImageView fx:id="imageview" fitHeight="100.0" fitWidth="111.0" layoutX="28.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true" />
      <ScrollPane fx:id="postScrollPane" layoutX="1.0" layoutY="180.0" prefHeight="319.0" prefWidth="800.0" styleClass="feed_container">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
      <ImageView fitHeight="41.0" fitWidth="32.0" layoutX="739.0" layoutY="25.0" onMouseClicked="#backToHome" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/back.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
   <stylesheets>
      <URL value="@css/profile.css" />
    </stylesheets>
</AnchorPane>
