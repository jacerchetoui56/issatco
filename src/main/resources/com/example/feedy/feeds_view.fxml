<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="all_container" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.feedy.controllers.FeedsController">
    <children>
        <Label layoutX="52.0" layoutY="13.0" prefHeight="30.0" prefWidth="96.0" text="ISSATCO">
            <font>
                <Font name="Microsoft YaHei Light" size="17.0" />
            </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
        </Label>
        <ImageView fitHeight="46.0" fitWidth="32.0" layoutX="14.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/twitter.png" />
            </image>
        </ImageView>
        <Label alignment="CENTER" layoutX="353.0" layoutY="84.0" prefHeight="30.0" prefWidth="96.0" styleClass="title" text="FEEDS">
            <font>
                <Font name="Microsoft YaHei Light" size="17.0" />
            </font>
        </Label>
        <Button fx:id="logoutButton" layoutX="732.0" layoutY="10.0" mnemonicParsing="false" onAction="#logout" styleClass="logoutbtn" text="Logout" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="577.0" layoutY="9.0" mnemonicParsing="false" onAction="#openCreatePostView" prefHeight="36.0" prefWidth="159.0" styleClass="new_post" text="Create a new post">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <ScrollPane fx:id="postScrollPane" layoutX="1.0" layoutY="134.0" prefHeight="364.0" prefWidth="801.0" style="-fx-background-color: #fff;" styleClass="feed_container">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="354.0" prefWidth="798.0" styleClass="feed_container">
               <children>

               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <Button layoutX="514.0" layoutY="10.0" mnemonicParsing="false" onAction="#openPesonalProfile" styleClass="profilebtn" text="Profile">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="417.0" layoutY="10.0" mnemonicParsing="false" onAction="#goToGroupChat" styleClass="group_chat" text="Group Chat">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
    </children>
    <stylesheets>
        <URL value="@css/feeds.css" />
    </stylesheets>
</AnchorPane>
