<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="all_container" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.feedy.controllers.PersonalProfileController">
   <children>
      <Label fx:id="usernameLabel" layoutX="162.0" layoutY="41.0" prefHeight="66.0" prefWidth="225.0" styleClass="profile_username" text="Label">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="bioLabel" layoutX="162.0" layoutY="107.0" text="Hello My name is jacer">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="726.0" layoutY="23.0" mnemonicParsing="false" onAction="#backToHome" styleClass="homebtn" text="Home" />
      <ImageView fx:id="imageview" fitHeight="100.0" fitWidth="111.0" layoutX="30.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true" />
      <ScrollPane fx:id="postScrollPane" layoutX="1.0" layoutY="180.0" prefHeight="319.0" prefWidth="800.0" styleClass="feed_container">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </ScrollPane>
      <Button layoutX="566.0" layoutY="23.0" mnemonicParsing="false" onAction="#openCreatePostView" prefHeight="36.0" prefWidth="159.0" styleClass="new_post" text="Create a new post">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
   <stylesheets>
      <URL value="@css/personal_profile.css" />
   </stylesheets>
</AnchorPane>
